#include <iostream>
using namespace std;
int main() {
const int M = 3; 
    const int N = 4; 

    int a[M][N] = {
        {10, 20, 30, 40},
        {10, 20, 30, 40},
        {10, 20, 30, 40},
    }; 

    
    for (int i = 0; i < M; i++) {
        for (int j = 0; j < N; j++) {
            cout << a[i][j] << " ";
        }
        cout << endl;
}
}
    
